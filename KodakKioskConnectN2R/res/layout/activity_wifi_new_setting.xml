<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >
    
    <include
        android:id="@+id/navigationbar"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        layout="@layout/navigationbar"
        android:focusable="false" />
  <LinearLayout 
    android:orientation="horizontal"
    android:layout_width="match_parent" 
    android:layout_height="match_parent"
    android:layout_below="@+id/navigationbar"
    
    android:baselineAligned="false">
  
    
    <fragment class="com.kodak.kodak_kioskconnect_n2r.fragments.WifiSettingLeftFragment"
        android:id="@+id/wifi_setting_left_fragment"
        android:layout_height="match_parent"
        android:layout_width="0dp"
        android:layout_weight="20"
        
        
        />
    
     <FrameLayout android:id="@+id/fragment_container" android:layout_weight="35"
            android:layout_width="0dp" android:layout_height="match_parent"
            />
     </LinearLayout> 
</RelativeLayout>
